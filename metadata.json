{
  "name": "puppet-hiera",
  "version": "4.0.1-rc0",
  "author": "Vox Pupuli",
  "summary": "Deploy hiera.yaml with hierarchy, and datadir",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-hiera",
  "project_page": "https://github.com/voxpupuli/puppet-hiera",
  "issues_url": "https://github.com/voxpupuli/puppet-hiera/issues",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.13.1 < 9.0.0"
    },
    {
      "name": "puppetlabs/inifile",
      "version_requirement": ">= 1.4.1 < 5.0.0"
    },
    {
      "name": "puppetlabs/puppetserver_gem",
      "version:requirement": ">= 1.0.0 < 2.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "16.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 5.5.8 < 7.0.0"
    }
  ]
}
